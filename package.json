{
  "name": "protheus-api",
  "version": "2.0.1",
  "main": "dist/server.js",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --watch src src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "lint": "eslint .",
    "test": "jest",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:pending": "mikro-orm migration:pending",
    "schema:update": "mikro-orm schema:update --run",
    "schema:fresh": "mikro-orm schema:fresh --run"
  },
  "keywords": [],
  "author": "In√°cio Rodrigues",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/caching": "^9.0.3",
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/redis": "^7.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@mikro-orm/core": "^6.4.14",
    "@mikro-orm/migrations": "^6.4.14",
    "@mikro-orm/postgresql": "^6.4.14",
    "@mikro-orm/reflection": "^6.4.14",
    "bull": "^4.16.5",
    "dotenv": "^16.5.0",
    "eslint-plugin-prettier": "^5.4.0",
    "fastify": "^5.3.2",
    "fastify-plugin": "^5.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "rotating-file-stream": "^3.2.6",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/parser": "^7.27.2",
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@mikro-orm/cli": "^6.4.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.16",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "msgpackr-extract"
    ],
    "onlyBuiltDependencies": [
      "msgpackr-extract"
    ]
  }
}
